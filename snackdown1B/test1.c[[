#include<bits/stdc++.h>
using namespace std;
int main(){
  int t,n,k;
  cin>>t;
  for(int i=0;i<t;i++){
    //cout<<"enter n and k\n";
    int cnt=0;
    cin>>n>>k;
    //cout<<"n,k="<<n<<" , "<<k;
    int v[n];
    for(int j=0;j<n;j++){
      cin>>v[j];
      if(v[j]!=1)
        cnt++;
    }
    //  cout<<"\nCout vector v=";
    //cout<<"\ncnt="<<cnt<<endl;
    //cout<<"enter anything\n";
    if(cnt<=k)
      cout<<"YES\n";
    else cout<<"NO\n";
  }
}
